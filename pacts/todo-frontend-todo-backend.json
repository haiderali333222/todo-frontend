{
  "consumer": {
    "name": "todo-frontend"
  },
  "provider": {
    "name": "todo-backend"
  },
  "interactions": [
    {
      "description": "a GET request to create a todo",
      "providerState": "provider allows todos creation",
      "request": {
        "method": "GET",
        "path": "/todo",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "_id": "1",
            "name": "Superman"
          }
        ],
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}